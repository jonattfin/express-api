{
  "name": "express-api",
  "version": "1.0.0",
  "description": "Express.Js starter kit / boilerplate with Babel, hot reloading, testing, linting and a working example app built in.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js --exec babel-node --presets babel-preset-env --plugins transform-object-rest-spread",
    "start:prod": "cross-env NODE_ENV=production npm run start",
    "lint": "eslint src --color",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:cover": "jest --runInBand --coverage"
  },
  "keywords": [],
  "author": "jonattfin@gmail.com",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.11.0",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "express-cache-controller": "^1.1.0",
    "express-validator": "^5.3.1",
    "firebase-admin": "^6.3.0",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.13",
    "moment": "^2.24.0",
    "mongoose": "^5.3.8",
    "morgan": "^1.9.1",
    "nconf": "^0.10.0",
    "nodemon": "^1.18.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sequelize": ">=5.3.0",
    "swagger-ui-express": "^3.0.10",
    "tedious": "^2.6.4",
    "uuid": "^3.3.2",
    "validator": "^10.9.0",
    "winston": "^3.1.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "jest": "^23.6.0",
    "supertest": "^3.3.0"
  }
